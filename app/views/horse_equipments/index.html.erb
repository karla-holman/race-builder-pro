<h1>Listing horse_equipments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horse_equipments.each do |horse_equipment| %>
      <tr>
        <td><%= link_to 'Show', horse_equipment %></td>
        <td><%= link_to 'Edit', edit_horse_equipment_path(horse_equipment) %></td>
        <td><%= link_to 'Destroy', horse_equipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

