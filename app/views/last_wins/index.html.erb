<h1>Listing last_wins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @last_wins.each do |last_win| %>
      <tr>
        <td><%= link_to 'Show', last_win %></td>
        <td><%= link_to 'Edit', edit_last_win_path(last_win) %></td>
        <td><%= link_to 'Destroy', last_win, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Last win', new_last_win_path %>
