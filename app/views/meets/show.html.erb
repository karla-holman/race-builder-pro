<div class="row">
	<ol class="breadcrumb">
	  <li><%= link_to 'Meet List', meets_path %></li>
	  <li class="active"><%= @meet.name %></li>
	</ol>
	<div class="col-xs-6">
		<div class="row">
			<div class="col-xs-12 ">
				<h1><%= @meet.name %></h1>
				<h4>Start Date: <%= @meet.start_date.strftime('%b %d, %Y') %>  
				End Date: <%= @meet.end_date.strftime('%b %d, %Y') %></h4>
			</div>
		</div>
	</div>
	<h2> Tels </h2>
	<div class="col-xs-6">
		<% @tels.each do |tel| %>
			<div class ="race-list-table">
				<table class="table table-condensed table-hover generic-table">
					<thead>
						<tr>
							<th>Weekend Of</th>
							<th colspan="1"></th>
						</tr>
					</thead>
					<tbody>
          	<tr>
            	<td><%= tel.weekend_start.strftime('%b %d, %Y') %></td>
            	<td><%= link_to 'View', tel, :class => 'btn btn-success btn-xs'%></td>
          	</tr>
			    </tbody>
				</table>
			</div>
		<% end %>
	</div>
</div>
