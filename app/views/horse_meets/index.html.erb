<h2>Listing horse_meets</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horse_meets.each do |horse_meet| %>
      <tr>
        <td><%= link_to 'Show', horse_meet %></td>
        <td><%= link_to 'Edit', edit_horse_meet_path(horse_meet) %></td>
        <td><%= link_to 'Destroy', horse_meet, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-danger btn-mini'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horse meet', new_horse_meet_path %>
