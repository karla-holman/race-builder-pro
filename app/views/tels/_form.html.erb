<%= form_for(@tel) do |f| %>
  <% if @tel.errors.any? %>
  <div class="row">
    <div class="col-xs-12">
      <div id="error_explanation">
        <h2><%= pluralize(@tel.errors.count, "error") %> prohibited this tel from being saved:</h2>
        <ul>
        <% @tel.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
    </div>
    <div class="col-xs-12 col-md-6">  
      <div class="form-group">
        <%= f.label 'Start Date' %>
        <%= date_field("tel", "start_date") %>
        <%= f.hidden_field :meet_id, :value => @meet.id %>
      </div>
      <div class="form-group">
        <%= f.label 'End Date' %>
        <%= date_field("tel", "end_date") %>
      </div>
      <div class="form-group">
        <%= f.label :week_number %>
        <%= f.select :week_number, options_for_select(0..100) %>
      </div>
      <div class="form-group">
        <%= f.submit :class => 'btn btn-success btn-sm'%>
        <%= link_to 'Back', @meet, :class => 'btn btn-danger btn-sm' %>
      </div>
    </div>
  </div>
<% end %>