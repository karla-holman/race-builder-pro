<h1>Listing condition_nodes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @condition_nodes.each do |condition_node| %>
      <tr>
        <td><%= link_to 'Show', condition_node %></td>
        <td><%= link_to 'Edit', edit_condition_node_path(condition_node) %></td>
        <td><%= link_to 'Destroy', condition_node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Condition node', new_condition_node_path %>
