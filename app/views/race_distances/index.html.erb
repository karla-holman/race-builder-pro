<h1>Listing race_distances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @race_distances.each do |race_distance| %>
      <tr>
        <td><%= link_to 'Show', race_distance %></td>
        <td><%= link_to 'Edit', edit_race_distance_path(race_distance) %></td>
        <td><%= link_to 'Destroy', race_distance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Race distance', new_race_distance_path %>
